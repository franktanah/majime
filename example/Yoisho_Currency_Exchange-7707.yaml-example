Base: "http://backend.yoisho.dob.jp/fx"
Tests:
 # GET - Get the FX rate for desired currency
 - path: "/currency?currency=USD"
   method: "GET"
   headers: ""
   expect-response: "200"
   expect-body: "json"
 # False positive - we want an error
 - path: "/currency?currency=XXX"
   method: "GET"
   headers: ""
   expect-response: "400"
   expect-body: "json"
